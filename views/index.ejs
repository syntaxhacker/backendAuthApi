<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Get all users</title>
  </head>
  <body>
    <h2>Users</h2>

    <ul>
      <% users.forEach(function(user) { %>
      <li><%= user.username %></li>
      <% }); %>
    </ul>
  </body>
</html>
